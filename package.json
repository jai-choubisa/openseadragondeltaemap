{
    "name": "openseadragontargetmap",
    "version": "0.0.1",
    "description": "Draws overlay over a targetscan, and shows calibration info",
    "main": "dist/openSeadragonTargetMap.js",
    "module": "module/openSeadragonTargetMap.js",
    "directories": {
        "test": "test"
    },
    "scripts": {
        "prebuild": "clear-folder dist module && prettier '{./{src,test}/*.{js,scss},./demo/*.{js,json}}' --write",
        "build": "rollup --config",
        "coverage": "clear-folder coverage && jest --coverage",
        "once": "npm run test && npm run build",
        "pretest": "eslint {src,test/**}/*.js",
        "test": "jest",
        "watch": "onchange --initial \"./{src,test/**}/*.{js,scss}\" \"./demo/*.{js,json}\" -- npm run once --silent"
    },
    "keywords": [
        "openseadragon",
        "plugin",
        "overlay",
        "patch",
        "target",
        "deltae",
        "front-end"
    ],
    "author": "Onno van der Zee",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/picturae/openseadragontargetmap.git"
    },
    "bugs": {
        "url": "https://github.com/picturae/openseadragontargetmap/issues"
    },
    "homepage": "https://github.com/picturae/openseadragontargetmap#readme",
    "peerDependencies": {
        "openseadragon": "^2.1.0"
    },
    "dependencies": {
        "openseadragon": "^2.4.2",
        "transform-case": "0.0.1"
    },
    "devDependencies": {
        "@babel/core": "^7.9.0",
        "@babel/preset-env": "^7.9.0",
        "@rollup/plugin-commonjs": "^11.0.2",
        "@rollup/plugin-node-resolve": "^7.1.1",
        "clear-folder": "^1.1.2",
        "coveralls": "^3.0.11",
        "eslint": "^6.5.1",
        "eslint-plugin-jasmine": "^2.10.1",
        "eslint-plugin-jest": "^22.20.0",
        "jest": "^25.1.0",
        "onchange": "^6.1.0",
        "prettier": "^1.18.2",
        "rollup": "^1.32.1",
        "rollup-plugin-babel": "^4.4.0",
        "rollup-plugin-sass": "^1.2.2",
        "rollup-plugin-terser": "^5.3.0"
    }
}
